<h1>Employee Form</h>

<%= form_for(@employee) do |f| %>
    <label>Name:</label><br>
    <%= f.text_field :first_name %><br>
    <%= f.text_field :last_name %><br>

    <label>Alias:</label><br>
    <%= f.text_field :alias %><br>

    <label>Title:</label><br>
    <%= f.text_field :title %><br>

    <label>Office:</label><br>
    <%= f.text_field :office %><br>

    <%= f.submit "Submit" %><br>
<% end %>